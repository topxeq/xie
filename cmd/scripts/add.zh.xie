// 将整数2入栈
push #i2

// 将整数5入栈
push #i5

// 将栈顶两个数值取出相加后结果压入栈中
// 此处使用了预定义全局变量$push
// 此时栈中应仅有一个数值（整数5）
add $push $pop $pop

// 输出栈顶数值（同时该数值被弹出）
plo $pop

// 将浮点数1.5与2.6相加后压栈
add $push #f1.5 #f2.6

// 弹栈输出
plo $pop

// 将两个字符串相加（连接）后赋值给变量c
add $c `abc` `123 456` 

// 输出变量c
plo $c

// 将变量c中的数值压栈
push $c

// 将字符串“9.18”压栈
push "9.18"

// 将栈顶两个字符串相加后赋值给变量d
add $d $pop $pop

// 输出变量d
plo $d

// 将整数18与190相加后，压入栈中
// 表示入栈操作
add $push #i18 #i190

// 弹栈输出
plo $pop