// 本例演示一个双重循环（二重嵌套循环）
// 相当于C/C++/Java语言的代码类似（由于语言差异，不一定完全一致。仅作示意）：
// for (int i = 0; i < 5; i ++) {
//   println(i, ":", i + 10);
//   for (int j = i; j < 10; j ++) {
//        println("j:", j);
//   }
// }

assign $i #i0

:loop1

    add $i #i10

    pln $i : $tmp

    assign $j $i

    :loop2

        pln j: $j

        inc $j

        < $j #i10

        if $tmp :loop2

    inc $i

    < $i #i5

    if $tmp :loop1
